# alias{{{
#

alias ls="ls -G -w"
alias la="ls -a"
alias lf="ls -f"
alias ll="ls -l"
alias lh="ls -lh"

alias ..="cd .."
alias ...="cd ../../"
alias ....="cd ../../../"
alias .....="cd ../../../../"
alias ......="cd ../../../../../"
